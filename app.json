{
  "expo": {
    "name": "VNue",
    "slug": "VNue",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/vnue-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#664DFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vnue.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vnue.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
